{
  "manifest_version": 2,
  "name": "LBS Helper",
  "version": "0.0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "images/icons/icon_16.png",
    "48": "images/icons/icon_48.png",
    "128": "images/icons/icon_128.png"
  },
  "permissions" : [
    "tabs",
    "background",
    "clipboardWrite"
  ],
  "page_action": {
    "default_icon": "images/icons/icon_16.png",
    "default_title": "Helper",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "index.html",
    "persistent": true
  }
}
